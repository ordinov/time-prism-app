{
  "name": "time-prism",
  "version": "0.2.0",
  "description": "Time tracking app with DAW-style timeline",
  "main": "dist/main/index.js",
  "scripts": {
    "dev": "rimraf ./dist && yarn build && concurrently \"npm run dev:vite\" \"npm run dev:electron\"",
    "build": "tsc -p tsconfig.main.json && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "dev:vite": "vite",
    "dev:electron": "wait-on http://localhost:5173 && cross-env NODE_ENV=development electron .",
    "build:electron": "electron-builder",
    "postinstall": "electron-rebuild"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@electron/rebuild": "^3.7.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/adm-zip": "^0.5.7",
    "@types/archiver": "^7.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/react": "^19.2.7",
    "@types/react-datepicker": "^6.2.0",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "jsdom": "^27.0.1",
    "postcss": "^8.5.6",
    "rimraf": "^6.1.2",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^6.4.1",
    "vitest": "^4.0.16",
    "wait-on": "^9.0.3"
  },
  "dependencies": {
    "adm-zip": "^0.5.16",
    "archiver": "^7.0.1",
    "better-sqlite3": "^12.5.0",
    "react": "^19.2.3",
    "react-datepicker": "^9.1.0",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.11.0"
  }
}
